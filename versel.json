{
    "version": 2,
    "builds": [
      {
        "src": "api/app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/post",
        "methods": ["POST"],
        "dest": "api/app.py"
      },
      {
        "src": "/",
        "methods": ["GET"],
        "dest": "api/app.py"
      }
    ]
  }
  